{
    "compilerOptions": {
        "outDir": "dist",
        "target": "ES6",
        "declaration": false,
        "noImplicitAny": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "sourceMap": true,
        "strictNullChecks": true,
        "allowSyntheticDefaultImports": true,
        "downlevelIteration": true,
        "module": "commonjs",
        "esModuleInterop": true
   },
} 